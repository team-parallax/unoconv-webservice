{
	"entryFile": "src/app.ts",
	"noImplicitAdditionalProperties": "throw-on-extras",
	"controllerPathGlobs": [
		"src/controller/**/*.ts"
	],
	"specMerging": "recursive",
	"spec": {
		"outputDirectory": "./",
		"specVersion": 3,
		"paths": {
			"/uploadtest": {
				"post": {
					"consumes": [
						"multipart/form-data"
					],
					"parameters": [
						{
							"in": "formData",
							"name": "randomFileIsHere",
							"required": true,
							"type": "file"
						}
					]
				}
			}
		}
	},
	"routes": {
		"middleware": "express",
		"routesDir": "src/routes"
	}
}